<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox fx:id="vbox" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="475.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.boardingpassgui.Controllers.GetFlights">
    <Label prefHeight="77.0" prefWidth="344.0" text="Boarding Pass Generator">
        <font>
            <Font size="30.0"/>
        </font>
    </Label>
    <Label alignment="CENTER" prefHeight="60.0" prefWidth="600.0"
           text="Here are the flights that meet your specifications." textAlignment="CENTER" wrapText="true">
        <VBox.margin>
            <Insets bottom="30.0"/>
        </VBox.margin>
    </Label>
    <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" maxWidth="350.0" prefHeight="232.0"
                prefWidth="600.0">
        <VBox fx:id="innerVBox" maxHeight="500.0" prefHeight="500.0">
        </VBox>
    </ScrollPane>
    <Button fx:id="button" onMouseClicked="#tryAnotherSearch" mnemonicParsing="false" text="Try another search">
        <VBox.margin>
            <Insets top="20.0"/>
        </VBox.margin>
    </Button>
</VBox>
